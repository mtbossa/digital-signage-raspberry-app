{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "public/**",
        "lib/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "raspberry-server#build": {
      "dependsOn": [
        "raspberry-frontend#build"
      ],
      "outputs": []
    }
  }
}