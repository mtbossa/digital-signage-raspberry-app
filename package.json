{
  "name": "intus",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:raspberry": "turbo run build --filter=@intus/raspberry-\\*",
    "build:docker": "turbo run build && docker build -f Dockerfile.raspberry -t mtbossa/raspberry-prod:staging .",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "prepare": "husky install"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.18"
}